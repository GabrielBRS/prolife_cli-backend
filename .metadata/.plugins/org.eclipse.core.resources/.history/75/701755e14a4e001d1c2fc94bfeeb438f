package com.gabrielsousa.prolife.domain.health;

import java.io.Serializable;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;
import javax.persistence.ManyToMany;

import com.gabrielsousa.prolife.domain.Doctor;
import com.gabrielsousa.prolife.domain.Patient;
import com.gabrielsousa.prolife.domain.Users;

@Entity
public class Anamnese implements Serializable {
	private static final long serialVersionUID = 1L;

	@Id
	@GeneratedValue(strategy=GenerationType.IDENTITY)
	private Integer id;

	@ManyToMany
	private Patient patient;
	
	@ManyToMany
	private Doctor doctor;
	
	public Anamnese() {
		
	}

	public Anamnese(Integer id, Users users5,Users users6) {
		super();
		this.id = id;
		this.patient = users5;
		this.doctor = users6;
	}



	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public Patient getPatient() {
		return patient;
	}

	public void setPatient(Patient patient) {
		this.patient = patient;
	}

	public Doctor getDoctor() {
		return doctor;
	}

	public void setDoctor(Doctor doctor) {
		this.doctor = doctor;
	}
}
